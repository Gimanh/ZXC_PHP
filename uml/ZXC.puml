@startuml

class Singleton{
    {static} - $instance = null;
    -  __construct()
    -  __clone()
    -  __wakeup()
    {static} +  getInstance()
}

class ZXC extends Singleton{
    - $version
    - $http
    - $logger
    - $router
    + initialize(array $config = [])
    
    +  go()
    +  writeLog($msg = '', $param = [])
    +  getVersion()
    +  getHttp()
    +  getLogger()
    +  getRouter()
}

@enduml